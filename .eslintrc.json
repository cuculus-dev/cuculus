{
  "extends": [
    "plugin:@next/next/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript",
    "next/core-web-vitals",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "ignorePatterns": [
    "node_modules/*",
    "next.config.js"
  ],
  "plugins": [
    "react",
    "react-hooks",
    "import"
  ],
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@mui/*/*/*"]
      }
    ]
  }
}
