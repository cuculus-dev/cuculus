{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "dependencyDashboard": false,
  "automerge": true,
  "platformAutomerge": true,
  "branchConcurrentLimit": 0,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "prBodyNotes": "@coderabbitai: ignore",
  "rangeStrategy": "replace",
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "rangeStrategy": "replace"
    }
  ]
}
